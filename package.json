  "scripts": {
    "start": "GATSBY_CONFIG_FILE=gatsby-config-dev.js gatsby develop",
    "develop": "GATSBY_CONFIG_FILE=gatsby-config-dev.js gatsby develop",
    "develop:wp": "GATSBY_CONFIG_FILE=gatsby-config-dev.js gatsby develop --verbose",
    "develop:ssl-bypass": "GATSBY_CONFIG_FILE=gatsby-config-ssl-bypass.js gatsby develop",
    "develop:mock": "bash ./scripts/enable-ssl-bypass.sh && GATSBY_CONFIG_FILE=gatsby-config-ssl-bypass.js gatsby develop",
    "ensure-dirs": "node ./scripts/ensure-directories.js",
    "build": "yarn ensure-dirs && gatsby build",
    "build:netlify": "yarn ensure-dirs && GATSBY_CONFIG_FILE=gatsby-config-ssl-bypass.js BYPASS_WORDPRESS=true gatsby build",
    "build:no-wp": "GATSBY_CONFIG_FILE=gatsby-config-ssl-bypass.js BYPASS_WORDPRESS=true gatsby build",
    "build:analyze": "ANALYZE=true gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean:wp": "gatsby clean && rm -rf wordpress-graphql-queries",
    "gatsby-provision": "gatsby-provision-contentful --contentful-data-path='./scripts/data.json' --space-id=$CONTENTFUL_SPACE_ID --management-token=$CONTENTFUL_MANAGEMENT_TOKEN",
    "setup": "node ./scripts/setup.js",
    "setup:wp": "node ./scripts/wordpress-setup.js",
    "setup:bypass": "bash ./scripts/enable-ssl-bypass.sh",
    "export-content": "contentful space export --space-id=$SPACE_ID --management-token=$MANAGEMENT_TOKEN --export-dir=scripts --content-file=data.json",
    "backup-and-provision": "node ./scripts/backup-and-provision.js",
    "backup:create": "node ./scripts/backup-and-provision.js",
    "backup:dry-run": "node ./scripts/backup-and-provision.js --dry-run",
    "video:verify": "node ./scripts/verify-video-setup.js",
    "video:setup-check": "node ./scripts/verify-video-setup.js",
    "wp:test-connection": "node ./scripts/test-wp-connection.js",
    "wp:sync": "node ./scripts/sync-wp-content.js",
    "wp:import-acf": "node ./scripts/import-acf-fields.js",
    "wp:fix-taxonomy": "node ./scripts/fix-taxonomy-issues.js",
    "wp:setup-about": "bash ./scripts/setup-about-page.sh",
    "status": "bash ./scripts/check-build-status.sh",
    "warnings:check": "bash ./scripts/check-warnings-status.sh",
    "blog:test": "bash ./scripts/test-blog-integration.sh",
    "blog:debug": "bash ./scripts/debug-blog-connection.sh",
    "about:check": "node ./scripts/check-about-page.js",
    "setup:phase3": "bash ./scripts/setup-phase-3.sh",
    "hero:develop": "GATSBY_CONFIG_FILE=gatsby-config-ssl-bypass.js gatsby develop",
    "hero:build": "GATSBY_CONFIG_FILE=gatsby-config-ssl-bypass.js gatsby build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
